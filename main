import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int heat;
        System.out.println("Lütfen hava sıcaklığını giriniz\t: ");
        heat = scanner.nextInt();
        if (heat <= 5) {
            System.out.println("Sıcaklık kayak yapmaya uygundur!");
        } else if (heat > 5 && heat <= 15) {
            System.out.println("Sıcaklık Sinemaya gitmeye uygundur!");
            if (heat > 10 && heat < 25) {
                System.out.println("sıcaklık Piknik yapmaya uygundur!");
            }
        } else {
            System.out.println("Sıcaklık yüzmeye uygundur");
        }
    }
}
